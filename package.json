{
  "name": "systemdify",
  "version": "0.2.0",
  "description": "Automatically create a systemd unit file for your Node application",
  "main": "index.js",
  "bin": "cli.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "test": "./node_modules/.bin/tape test/*.js | node_modules/.bin/tap-spec"
  },
  "keywords": [
    "systemd",
    "daemon",
    "forever",
    "restart",
    "linux"
  ],
  "author": "Gilles De Mey <gilles.de.mey@gmail.com>",
  "repository": "gillesdemey/systemdify",
  "license": "MIT",
  "devDependencies": {
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0"
  },
  "dependencies": {
    "meow": "^3.6.0",
    "npmlog": "^2.0.0",
    "which": "^1.2.1",
    "xtend": "^4.0.1"
  }
}
